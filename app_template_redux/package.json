{
  "name": "@canadahelps/<name_of_app_should_match_folder_name>",
  "version": "<x>.<x>.<x>",
  "description": "<description>",
  "main": "src/index<.ts|.js|.tsx>",
  "scripts": {
    "prepublishOnly": "npm test",
    "prepare": "run-s clean transpile",
    "clean": "rimraf lib",
    "copy-style": "cpx \"src/**/*.{css,scss}\" lib",
    "transpile": "run-p transpile:babel transpile:declarations copy-style",
    "transpile:babel": "babel src --out-dir lib --extensions \".ts,.tsx,.js,.jsx\"",
    "transpile:declarations": "tsc --emitDeclarationOnly",
    "build-storybook": "build-storybook",
    "build:dev": "webpack --env.mode development",
    "build": "webpack --env.mode production --display-error-details",
    "deploy:dev": "npm run build:dev",
    "deploy": "npm run build",
    "storybook": "start-storybook -s ../../dist -p 9009",
    "test:ci": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --ci",
    "test:coverage": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --coverage",
    "test:snapshots": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --updateSnapshot",
    "test": "cross-env DEBUG_PRINT_LIMIT=30000 NODE_ICU_DATA=./node_modules/full-icu jest",
    "watch:test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --watch",
    "watch": "webpack --env.mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CanadaHelps/CHOUI.git",
    "directory": "<specific_directory_in_repo>"
  },
  "author": "CanadaHelps",
  "private": true,
  "license": "UNLICENSED",
  "dependencies": {
    "@canadahelps/chui": "^1.14.2",
    "@canadahelps/chutils": "^1.15.0",
    "@canadahelps/ch-datatable-toolkit": "^2.3.0",
    "@canadahelps/localization": "^1.1.0",
    "@reduxjs/toolkit": "^1.5.1",
    "cnbuilder": "^2.6.0",
    "css-loader": "1.0.0",
    "date-fns": "^2.19.0",
    "date-fns-tz": "^1.1.13",
    "history": "^5.0.0",
    "isomorphic-fetch": "^3.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@canadahelps/ui-common-config": "^2.1.2",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/node-logger": "^6.1.15",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.15",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^11.2.6",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "full-icu": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "react-axe": "^3.5.3",
    "react-select-event": "^5.3.0",
    "redux-mock-store": "^1.5.4"
  }
}
